{
  "name": "pfamserver",
  "version": "1.0.0",
  "description": "PfamServer graphical fronend",
  "devDependencies": {
    "browser-sync": "^2.9.6",
    "protractor": "5.3.1",
    "v8-profiler": "^5.7.0",
    "v8-profiler-node8": "^5.7.7",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "start": "NODE_ENV=webpack-dev-server node_modules/.bin/webpack-dev-server --progress --colors --port 2992 --content-base pfamserver/static/dist",
    "build": "node_modules/.bin/webpack --progress --profile --colors",
    "test-e2e": "./node_modules/protractor/bin/protractor web/test/protractor.conf.js",
    "webdriver-update": "./node_modules/protractor/bin/webdriver-manager update",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js web/src/client/",
    "build-stats": "./node_modules/webpack/bin/webpack.js --profile --json > stats.json",
    "postinstall": "NODE_ENV=production node_modules/.bin/webpack --progress --profile --colors"
  },
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "bio-pv": "^1.8.1",
    "biojs-vis-seqlogo": "0.0.14",
    "breakpoint-sass": "^2.7.1",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-switch": "^3.3.4",
    "clean-webpack-plugin": "^0.1.16",
    "crossroads": "0.12.2",
    "css-loader": "^0.28.7",
    "cytoscape": "^3.2.0",
    "cytoscape-cola": "^2.0.0",
    "cytoscape-cose-bilkent": "^2.0.1",
    "cytoscape-markov-cluster": "git+https://github.com/cytoscape/cytoscape.js-markov-cluster.git",
    "d3": "^3.5.0",
    "data-collection": "^1.1.6",
    "dotenv": "2.0.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-saver": "^1.3.3",
    "golden-layout": "^1.5.8",
    "hasher": "1.2.0",
    "html-loader": "^0.5.1",
    "html-minify-loader": "^1.1.0",
    "image-webpack-loader": "^1.4.0",
    "img-loader": "^2.0.0",
    "ion-rangeslider": "^2.2.0",
    "jquery": "^2.2.0",
    "jquery-easing": "0.0.1",
    "jquery-typeahead": "^2.8.0",
    "jquery-ui": "1.13.0",
    "jquery.easing": "^1.3.2",
    "jr-qrcode": "1.0.5",
    "json2csv": "^3.10.0",
    "jszip": "^3.1.4",
    "knockout": "3.4.1",
    "knockout-mapping": "2.6.0",
    "knockout-template-loader": "1.0.1",
    "knockout.bootstrap.modal": "2.0.0",
    "less": "^2.3.1",
    "less-loader": "^4.0.3",
    "lodash": "^4.13.1",
    "manifest-revision-webpack-plugin": "^0.4.0",
    "merge-files-webpack-plugin": "^1.1.2",
    "mini-toastr": "0.5.0",
    "node-gyp": "^3.6.2",
    "node-sass": "^7.0.0",
    "npm": "^5.6.0",
    "postcss-loader": "^2.0.10",
    "precss": "^3.1.0",
    "prepack-webpack-plugin": "1.1.0",
    "postinstall": "*",
    "raw-loader": "^0.5.1",
    "requirejs": "2.3.2",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "sequence-logo": "0.0.2",
    "sitemap-webpack-plugin": "^0.5.1",
    "sprintf-js": "^1.0.3",
    "style-loader": "^0.18.1",
    "svgpath": "^2.2.1",
    "sync-exec": "^0.6.2",
    "typeahead": "^0.2.2",
    "uglify-js": "^2.8.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-join": "^1.1.0",
    "url-loader": "0.5.8",
    "webpack": "^2.7.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true,
      "amd": true,
      "jasmine": true
    },
    "globals": {
      "CONFIG": true,
      "$": true
    },
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "semi": [
        "error"
      ],
      "indent": [
        "error",
        4
      ],
      "quote": [
        "single"
      ],
      "eqeqeq": [
        "off"
      ],
      "camelcase": [
        "off"
      ],
      "no-mixed-operators": [
        "off"
      ],
      "no-unused-vars": [
        "warn",
        {
          "args": "all"
        }
      ],
      "one-var": [
        "off"
      ],
      "no-console": "warn",
      "handle-callback-err": [
        "off"
      ],
      "import/no-webpack-loader-syntax": [
        "off"
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/*",
    "web/src/tests/*",
    "**/static/js/",
    "webpack.config.js"
  ]
}
